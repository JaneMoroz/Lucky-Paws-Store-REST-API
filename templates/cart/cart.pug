.cart
  h1.heading-primary--main Your Cart
  ul(class="cart__list")
    li.list-el
      div(class="list-el__details-box")
        img(src="/img/products/product-5.jpg", alt="Product 5")
        a.btn.btn-text.btn-text--small(href="#") Delicious Snacks
        span(class="list-el__last-span") $19.99
      div(class="list-el__details-btns")
        a.btn.btn-icon(href="#")
          svg(xmlns="http://www.w3.org/2000/svg", fill="none", viewBox="0 0 24 24", stroke="currentColor")
            path(stroke-linecap="round", stroke-linejoin="round", d="M19 7l-.867 12.142A2 2 0 0116.138 21H7.862a2 2 0 01-1.995-1.858L5 7m5 4v6m4-6v6m1-10V4a1 1 0 00-1-1h-4a1 1 0 00-1 1v3M4 7h16")
  div(class="cart__prices")
    div
      span Subtotal:
      span $110
    div
      span Taxes:
      span $10
    div
      span Shipping price:
      span $20
    div(class="cart__prices--total")
      span Total:
      span $140
  div(class="cart__form")
    form.form.form--big(action="#")
      div(class="form__group")
        input#address(type="text", class="form__input", placeholder="Address", required)
      div(class="form__group")
        a.btn.btn-color.btn-color--mixed  Continue with Payment 